					<xs:documentation>Il codice ISTAT del comune a cui inviare la risposta.</xs:documentation>
				</xs:annotation></xs:element>
			<xs:element name="operazioneRichiesta" 	type="tipoOperazioneRichiesta"	minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Codice dell'operazione richiesta (es. 1001,5005)</xs:documentation>
				</xs:annotation></xs:element>			
			<xs:element name="dataOraRichiesta" 	type="xs:dateTime"      		minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>La data e l‟ora in cui si invia la comunicazione anagrafica ad ANPR.
</xs:documentation>
				</xs:annotation></xs:element>
			<xs:element name="protocolloComune" 	type="charMax20" 				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>La data e l‟ora in cui si invia la comunicazione anagrafica ad ANPR.</xs:documentation>
				</xs:annotation></xs:element>				
			<xs:element name="dataProtocolloComune" type="xs:date" 					minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Data di protocollazione della pratica da parte dell'ufficio Anagrafe.</xs:documentation>
				</xs:annotation></xs:element>
			<xs:element name="dataDecorrenza" 		type="xs:date" 					minOccurs="0" maxOccurs="1">
				<xs:annotation>
