<!--  alfanumericoComune massimo 80 caratteri -->
	<xs:simpleType name="tipoOperazioneComune">
		<xs:restriction base="xs:string">
			<!-- <xs:pattern value="^(?!0*(\.0+)?$)(\d+|\d*\.\d+)$"/> -->
			<xs:pattern value="^\d*[1-9]\d*$"/>
		</xs:restriction>
	</xs:simpleType>
