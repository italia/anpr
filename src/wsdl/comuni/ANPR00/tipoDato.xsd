		</xs:restriction>
	</xs:simpleType>	
	
	<xs:simpleType name="tipoDenominazioneEstero">
		<xs:restriction base="xs:string">
			<xs:pattern value="([0-9&#73;&#105;&#138;&#140;&#142;&#154;&#156;&#158;-&#159;&#192;-&#382;&#393;a-zA-Z\- &apos;/.,()&#35;&#58;])*"/>
			<xs:minLength value="1"/>
			<xs:maxLength value="120"/>
		</xs:restriction>
	</xs:simpleType>	
	
	<!-- gestione carattere slash -->
	<xs:simpleType name="alfanumericoslashbase">
		<xs:restriction base="xs:string">
			<xs:pattern value="([0-9&#73;&#105;&#138;&#140;&#142;&#154;&#156;&#158;-&#159;&#192;-&#382;&#393;a-zA-Z/\- &apos;/.,()])*"/>
		</xs:restriction>
	</xs:simpleType>		
	<xs:simpleType name="alfanumericoslash120char">
		<xs:restriction base="alfanumericoslashbase">
			<xs:minLength value="1"/>
			<xs:maxLength value="120"/>
		</xs:restriction>
	</xs:simpleType>	
	
