                    <xs:documentation>La denominazione del comune</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="codiceIstat" type="tipoCodiceIstatComune" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Il codice istat del comune</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="siglaProvinciaIstat" type="alfabeticoProvincia" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>La sigla istat della provincia di appartenenza del comune</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="descrizioneLocalita" type="alfanumerico120char" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Descrizione della località italiana.
                        Non deve essere indicata se coincide con nomeComune.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
