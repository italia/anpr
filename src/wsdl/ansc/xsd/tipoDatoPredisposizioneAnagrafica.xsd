    <xs:complexType name="tipoDatoTestata">
        <xs:sequence>
            <xs:element name="idNotifica" type="tipoIdNotifica" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Identificativo della predisposizione anagrafica
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="idTipoContenuto" type="numerico1" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>1=ATTO|2=ANNOTAZIONE|3=TRASCRIZIONE|4=ANNOTAZIONE AUTOMATICA
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="idTipoEvento" type="numerico1" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>1=NASCITA|2=MORTE|3=MATRIMONIO|4=UNIONE CIVILE|5=CITTADINANZA
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="codMittente" type="tipoCodMittente" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Il codice ISTAT del comune che effettua la richiesta.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="idTipoNotifica"
                        type="tipoNotifica" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Identificativo notifica (es. SC01, SC02..)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dataValidita" type="xs:date"
                        minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Data validita dell'atto.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="idUsecase"
                        type="alfanumerico30char" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>ID caso d'uso</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="descrizioneUsecase"
                        type="charMax600" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Descrizione caso d'uso</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

