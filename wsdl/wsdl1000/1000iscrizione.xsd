					<xs:element name="unioneCivile" type="anpr:tipoUnioneCivile" minOccurs="0" maxOccurs="1"></xs:element>
					<xs:element name="cartaIdentita"
						type="anpr:tipoCartaIdentita" minOccurs="0" maxOccurs="1" />
