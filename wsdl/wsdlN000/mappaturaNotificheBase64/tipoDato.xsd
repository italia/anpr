	<xs:simpleType name="tipoMotivoIscrizioneAire">		
		<xs:annotation>
			<xs:documentation>
				CO1=Espatrio e/o residenza all' estero, C02=Nascita (stato civile), se figlio di iscritto all'A.I.R.E.,
				C03=Trasferimento dall' A.I.R.E. di un altro comune,C04=Reiscrizione da irreperibilita', se residente all' estero,
				C05=Acquisizione della cittadinanza italiana, se residente all'estero, C06=Espatrio, C07=legge n. 15 del 16/01/1992, Art. 11, 
				C09=Altro
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="charMax3">
			<!-- <xs:pattern value="C01|C02|CO3|C04|CO5|C06|C07|C09"/> -->			
		</xs:restriction>
	</xs:simpleType>
